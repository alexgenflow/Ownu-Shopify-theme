        "f9602b8e-12b0-44ac-a1a6-98cafc6de3c4": {
          "type": "html",
          "settings": {
            "html_content": "<div class=\"page-width\">\n  <div class=\"customer\" id=\"StudentVerificationForm\">\n      <div class=\"field\">\n        <input type=\"email\" value=\"\" name=\"email\" id=\"StudentVerifyEmail\" autocorrect=\"off\" autocapitalize=\"off\" autocomplete=\"email\" placeholder=\"test@example.com\">\n        <label for=\"StudentVerifyEmail\">\n          Email\n        <\/label>\n      <\/div>\n      <button id=\"StudentVerifySubmit\">Verify Email<\/button>\n  <\/div>\n  <div id=\"verification-error\" style=\"display: none;\">\n    Unfortunately we could not verify you at this email, please make sure you use your student email address!\n  <\/div>\n  <div id=\"verification-success\" style=\"display: none;\">\n    Congratulations! Check your email at <span id=\"SuccessMessageEmailAddress\"><\/span> for your discount code!\n  <\/div>\n  <span id=\"DiscountCodeText\"><\/span>\n  <\/div>\n\n  <script>\n  (global => {\n    console.log('test');\n    window.addEventListener('DOMContentLoaded', (_event) => {\n      const button = document.getElementById('StudentVerifySubmit');\n      const form = document.getElementById('StudentVerificationForm');\n      button.addEventListener(\"click\", () => { \n        const email = document.getElementById('StudentVerifyEmail').value;\n        const http = new XMLHttpRequest();\n        button.disabled = true;\n        http.onreadystatechange = () => { \n          if (http.readyState === 4 && http.status === 200) {\n            form.style.display = 'none';\n            document.getElementById('verification-success').style.display = 'block';\n            document.getElementById('SuccessMessageEmailAddress').innerText = email;\n          } else if (http.readyState === 4) {\n            document.getElementById('verification-error').style.display = 'block';\n            form.style.display = 'none';\n          }\n        }\n        http.open(\"GET\", 'https:\/\/x0bkits8j3.execute-api.eu-west-1.amazonaws.com\/prod\/verify\/email\/8ecc5861-cebf-47db-a78f-f876b8ab453c\/STUDENT_VERIFICATION\/' + email, true);\n        http.send(null);   \n  \n    });\n    \n    const getQsByName = (name, url = global.location.href) => {\n      name = name.replace(\/[\\[\\]]\/g, '\\\\$&');\n      const regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)'),\n        results = regex.exec(url);\n      if (!results) return null;\n      if (!results[2]) return '';\n      return decodeURIComponent(results[2].replace(\/\\+\/g, ' '));\n    };\n\n    const code = getQsByName('code');\n    if (code) {\n      console.log('code', code);\nform.style.display = 'none';\n      document.getElementById('DiscountCodeText').innerText = 'Your student discount code is: ' + code;\n    }\n  });\n})(window);\n<\/script>",
            "hidden_mobile": false
          }
